
export int testComparisonOperatorSequence(int value) {
	// Check all possibilities!
	value = value < value < value;
	value = value < value <= value;
	value = value < value > value;
	value = value < value >= value;
	value = value < value == value;
	value = value < value != value;
	
	value = value <= value < value;
	value = value <= value <= value;
	value = value <= value > value;
	value = value <= value >= value;
	value = value <= value == value;
	value = value <= value != value;
	
	value = value > value < value;
	value = value > value <= value;
	value = value > value > value;
	value = value > value >= value;
	value = value > value == value;
	value = value > value != value;
	
	value = value >= value < value;
	value = value >= value <= value;
	value = value >= value > value;
	value = value >= value >= value;
	value = value >= value == value;
	value = value >= value != value;
	
	value = value == value < value;
	value = value == value <= value;
	value = value == value > value;
	value = value == value >= value;
	value = value == value == value;
	value = value == value != value;
	
	value = value != value < value;
	value = value != value <= value;
	value = value != value > value;
	value = value != value >= value;
	value = value != value == value;
	value = value != value != value;
	
	return value;
}
